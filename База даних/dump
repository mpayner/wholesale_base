  `phone_number` char(13) NOT NULL,
  `type_id` int NOT NULL,
  `address` varchar(255) DEFAULT NULL,
  `base_id` int unsigned DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `password_UNIQUE` (`password`),
  UNIQUE KEY `phone_number_UNIQUE` (`phone_number`),
  UNIQUE KEY `email_UNIQUE` (`email`),
  KEY `fk_user_user_type_idx` (`type_id`),
  KEY `fk_user_base1_idx` (`base_id`),
  KEY `name_index` (`name`),
  CONSTRAINT `fk_user_base1` FOREIGN KEY (`base_id`) REFERENCES `base` (`id`),
  CONSTRAINT `fk_user_user_type` FOREIGN KEY (`type_id`) REFERENCES `user_type` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=utf8mb3;
