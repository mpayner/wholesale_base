DROP TABLE IF EXISTS `order`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!50503 SET character_set_client = utf8mb4 */;
CREATE TABLE `order` (
  `id` int unsigned NOT NULL AUTO_INCREMENT,
  `customer_id` int unsigned NOT NULL,
  `distributor_id` int unsigned NOT NULL,
  `waybill` varchar(45) NOT NULL,
  `create_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `status_id` tinyint unsigned NOT NULL DEFAULT '1',
  `address` varchar(255) DEFAULT NULL,
  `delivery_date` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_base_distributor_order_user1_idx` (`distributor_id`),
  KEY `fk_base_distributor_order_user2_idx` (`customer_id`),
  KEY `fk_order_order_status1_idx` (`status_id`),
  CONSTRAINT `fk_base_distributor_order_user1` FOREIGN KEY (`distributor_id`) REFERENCES `user` (`id`),
  CONSTRAINT `fk_base_distributor_order_user2` FOREIGN KEY (`customer_id`) REFERENCES `user` (`id`),
  CONSTRAINT `fk_order_order_status1` FOREIGN KEY (`status_id`) REFERENCES `order_status` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=78152 DEFAULT CHARSET=utf8mb3 COMMENT='	';
